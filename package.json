{
  "name": "ddn-wallet",
  "version": "2.2.0",
  "description": "A wallet for managing ddn assets",
  "private": true,
  "main": "main.js",
  "scripts": {
    "precommitbk": "npm run lint-staged",
    "presite": "npm run generate-mock && cd functions && npm install",
    "starts": "cross-env APP_TYPE=site umi dev",
    "starts:no-mock": "cross-env MOCK=none umi dev",
    "start": "cross-env UMI_UI=1 umi dev",
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "start:no-ui": "cross-env UMI_UI=none umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
    "build": "umi build",
    "site": "npm run presite && cross-env APP_TYPE=site npm run build && firebase deploy",
    "analyze": "cross-env ANALYZE=1 umi build",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx  src mock tests && npm run lint:style",
    "lint:fix": "eslint --fix --ext .js,.jsx,.ts,.tsx src mock tests && npm run lint:style",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx",
    "test": "umi test",
    "generate-mock": "node ./scripts/generateMock.js",
    "test:component": "umi test ./src/components",
    "test:all": "node ./tests/run-tests.js",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "dependencies": {
    "@ant-design/compatible": "^1.0.8",
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-form": "^1.24.2",
    "@ant-design/pro-layout": "^6.18.2",
    "@antv/data-set": "^0.9.6",
    "@babel/runtime": "^7.0.0",
    "@ddn/js-sdk": "^1.0.7",
    "@umijs/ui-theme": "^1.0.1-beta.1",
    "antd": "^4.15.6",
    "bip39": "^2.5.0",
    "bizcharts": "^3.2.2",
    "bizcharts-plugin-slider": "^2.0.3",
    "classnames": "^2.2.6",
    "copy-to-clipboard": "^3.0.8",
    "dva": "^2.4.0",
    "elliptic": "^6.5.4",
    "enquire-js": "^0.2.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "hash.js": "^1.1.5",
    "jdenticon": "^2.1.0",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^4.0.0",
    "module": "^1.2.5",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "nzh": "^1.0.3",
    "omit.js": "^1.0.0",
    "path-to-regexp": "^2.4.0",
    "prop-types": "^15.5.10",
    "qrcode": "^1.3.0",
    "qs": "^6.5.2",
    "rc-animate": "^2.4.4",
    "react": "17.x",
    "react-container-query": "^0.11.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "17.x",
    "react-fittext": "^1.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "tweetnacl": "^1.0.3",
    "umi-request": "^1.3.6",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.4.11",
    "@types/react-dom": "^16.0.6",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@umijs/preset-react": "1.x",
    "@umijs/preset-ui": "^2.2.9",
    "antd-pro-merge-less": "^0.0.9",
    "antd-pro-theme-webpack-plugin": "^1.1.8",
    "babel-eslint": "^9.0.0",
    "cross-env": "^5.1.1",
    "cross-port-killer": "^1.0.1",
    "enzyme": "^3.4.4",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-compat": "^2.5.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "gh-pages": "^2.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "merge-umi-mock-data": "^0.0.3",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^2.3.0",
    "pro-download": "^1.0.1",
    "stylelint": "^9.4.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.0.0",
    "typescript": "^4.2.4",
    "umi": "^3.4.21",
    "umi-plugin-ga": "^1.0.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,less}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": "npm run lint-staged:js",
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "checkFiles": [
    "src/**/*.js*",
    "src/**/*.ts*",
    "src/**/*.less",
    "config/**/*.js*",
    "scripts/**/*.js"
  ]
}
